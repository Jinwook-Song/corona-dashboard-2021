   "source": [
    "import pandas as pd\n",
    "\n",
    "# get daily data\n",
    "daily_df = pd.read_csv('data/daily-report.csv')\n",
    "\n",
    "# data of each country\n",
    "countries_df = daily_df[['Country_Region','Confirmed','Deaths','Recovered']]\n",
    "countries_df = countries_df.groupby('Country_Region').sum().reset_index()\n",
    "\n",
    "countries_df"
   ]
